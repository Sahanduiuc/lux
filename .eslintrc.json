{
  "rules": {
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "strict": 2,
    "angular/no-digest": 0,
    "angular/di": [
      2,
      "array"
    ]
  },
  "env": {
    "browser": true,
    "amd": true
  },
  "plugins": [
    "disable"
  ],
  "extends": [
    "eslint:recommended",
    "angular"
  ],
  "globals": {
    "module": true,
    "inject": true
  }
}
